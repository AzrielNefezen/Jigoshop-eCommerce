jQuery ($) ->
  $('input[type=checkbox].switch-medium').bootstrapSwitch
    size: 'small'
    onText: "Yes"
    offText: "No"
  $('input[type=checkbox].switch-mini').bootstrapSwitch
    size: 'mini'
    onText: "Yes"
    offText: "No"
  $('input[type=checkbox].switch-normal').bootstrapSwitch
    size: 'normal'
    onText: "Yes"
    offText: "No"